<div>
  	<table class="table">
  		<thead class="thead-light">
  		<tr>
	  		<th>Title</th>
	  		<th>Description</th>
	  		<th>Date</th>
	  		<th>Amount</th>
  		</tr>
  	</thead>

  		<% @expense.each do |e| %>
  		<tr>
  			<td><%= e.title %></td>
  			<td><%= e.description %></td>
  			<td><%= e.date %></td>
  			<td><%= e.amount %></td>
  		</tr>
  		<%end%>
  	</table>
  </div>